use strict;
use lib qw(/usr/lib/libDrakX);

# i18n: IMPORTANT: to get correct namespace (drakx-net instead of libDrakX)
BEGIN { unshift @::textdomains, 'drakx-net' }
use standalone;
use interactive;
use network::drakvpn;

my $in = 'interactive'->vnew('su');
network::drakvpn::create_connection($in);
